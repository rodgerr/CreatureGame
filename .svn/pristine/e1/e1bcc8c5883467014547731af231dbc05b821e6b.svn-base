package graph;

public class RunTest {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		GraphRendererFrame renderer = new GraphRendererFrame("ms");
		
		while(true){
			renderer.renderValue((int) (Math.random()*100));
			try {
				Thread.sleep(1000);
			} catch (InterruptedException e) {
